
        $sql = "SELECT idEgzemplarza, stan, cena, idPlacówki FROM Egzemplarze;";
        $result = $conn->query($sql);
        if ($result->num_rows > 0) {
            while ($row = $result->fetch_assoc()) {
                echo "<tr><td>" . $row["idEgzemplarza"] . "</td><td>" . $row["stan"] . "</td><td>" . $row["cena"] . "</td><td>" . $row["idPlacówki"] . "</td></tr>";
            }
        } else {
            echo "0 results";
        }